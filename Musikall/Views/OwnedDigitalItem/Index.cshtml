@model IEnumerable<Musikall.Models.OwnedDigitalItem>

@{
    ViewBag.Title = "My digital music";
    ViewBag.isMyDigital = true;
}

@Html.Partial("_AccountPartial")


@if (Model.Count() == 0)
{
    <h3>No Music</h3>
}
else
{
    <table>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Item.ItemName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Item.DLUrl)
            </th>
        </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Item.ItemName)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", "Item", new { id = item.Id }, null)
            </td>
            <td>
                @Html.ActionLink("Download", "Download", "Item", new { id = item.Id }, null)
            </td>
        </tr>
    }

    </table>
}